{
  "name": "etax-web-new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "firebase:deploy": "firebase deploy",
    "test:e2e": "node tools/smoke.test.js",
    "test:e2e:dev": "concurrently -k -s first -n DEV,TEST \"npm run dev\" \"wait-on http://localhost:3000 && node tools/smoke.test.js\"",
    "test:e2e:vitest": "vitest run __tests__/e2e",
    "diagram:svg": "mmdc -p tools/puppeteer.config.json -i docs/diagrams/etax_navigation.mmd -o docs/diagrams/etax_navigation.svg -b transparent -t neutral",
    "diagram:png": "mmdc -p tools/puppeteer.config.json -i docs/diagrams/etax_navigation.mmd -o docs/diagrams/etax_navigation.png -b transparent -t neutral",
    "diagram:build": "npm run diagram:svg && npm run diagram:png",
    "assets:webp": "node tools/optimize_assets.js public/assets"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.6",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "firebase": "^10.13.2",
    "firebase-admin": "^12.6.0",
    "jimp": "^0.22.12",
    "jose": "^6.1.0",
    "lucide-react": "^0.548.0",
    "next": "16.0.1",
    "pdf-lib": "^1.17.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tesseract.js": "^6.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^10.9.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "concurrently": "^9.0.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "jsdom": "^27.1.0",
    "puppeteer": "^23.8.0",
    "sharp": "^0.34.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.6",
    "wait-on": "^8.0.0"
  }
}
